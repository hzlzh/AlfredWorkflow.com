### From qWorkflow
on q_split(s, delim)
	set oldDelims to AppleScript's text item delimiters
	set AppleScript's text item delimiters to delim
	set output to text items of s
	set AppleScript's text item delimiters to oldDelims
	return output
end q_split

set listIndexAndTask to "{query}" as text

set {listIndex, task} to q_split(listIndexAndTask, "::")

tell wllib
	addTaskToList(listIndex, task)
end tell</string>
