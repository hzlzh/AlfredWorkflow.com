				<key>script</key>
				<string>require_once('alfredtweet.php');
require_once('workflows.php');

$at = new AlfredTweet();
$w  = new Workflows();

$mentions = $at-&gt;mentions();
$username = $at-&gt;whoami();

foreach( $mentions as $mention ):
	$users = array();
	foreach( $mention-&gt;entities-&gt;user_mentions as $user ):
		if ( $user-&gt;screen_name != $username ):
			array_push( $users, '@'.$user-&gt;screen_name );
		endif;
	endforeach;
	array_push( $users, '@'.$mention-&gt;user-&gt;screen_name );
	$users = implode( " ", $users );
	$uid = exec( 'uuidgen' );
	$w-&gt;result( 'alfredtweet.'.$uid, $users.' ', $mention-&gt;user-&gt;screen_name, $mention-&gt;text, 'icon.png' );
endforeach;

echo $w-&gt;toxml();</string>
