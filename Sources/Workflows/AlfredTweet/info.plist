$user = $temp[0];
if ( isset( $temp[1] ) ):
	$message = array_slice( $temp, 1 );
	$message = implode( ' ', $message );
else:
	$message = '';
endif; 
if ( isset( $user ) &amp;&amp; isset( $message ) &amp;&amp; $message != '' ):
	$w-&gt;result( 'dm', $user.' '.$message.' ', 'Send Direcet Message', 'Send '.$message. ' to '.$user, 'icon.png' );
else:
	$w-&gt;result( 'dm', '', 'Send Direcet Message', 'Send '.$message. ' to '.$user, 'icon.png', 'no' );
endif;

if ( count( $in ) == 1 ):
	if ( preg_match( '^@[a-zA-Z0-9-_]{2,}^' , $in[0] ) ):
		$search = substr( $in[0], 1 );
		$matches = $db-&gt;select('*')-&gt;from('users')-&gt;like( 'handle', $search )-&gt;get();
		foreach( $matches as $match ):
			$w-&gt;result('autoc', '@'.$match-&gt;handle, $match-&gt;handle, '', 'icon.png', 'no', '@'.$match-&gt;handle.' ' );
		endforeach;
	endif;
endif;

echo $w-&gt;toxml();</string>
