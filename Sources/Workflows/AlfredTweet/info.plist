				<key>script</key>
				<string>require('workflows.php');
require('alfredtweet.php');
require('sqlitedb.php');

$w = new Workflows();
$at = new AlfredTweet();
$db = new SQLiteDB('autocomplete.db');

$table = array(
	'handle' =&gt; 'text PRIMARY KEY'
);

$db-&gt;create_table( $table, 'users' );
$db-&gt;truncate( 'users' );

$temp = $at-&gt;friends();
$friends = array();
foreach( $temp-&gt;ids as $friend ):
	array_push( $friends, $friend );
endforeach;

$temp = $at-&gt;followers();
$followers = array();
foreach( $temp-&gt;ids as $follower ):
	array_push( $followers, $follower );
endforeach;

$friends = array_chunk( $friends, 100 );
$followers = array_chunk( $followers, 100 );

foreach( $friends as $set ):
	$list = implode( ',', $set );
	$lump = $at-&gt;lookup( $list );
	foreach( $lump as $user ):
		$user = array(
			'handle' =&gt; $user-&gt;screen_name
		);
		$db-&gt;insert( $user, 'users' );
	endforeach;
endforeach;

foreach( $followers as $set ):
	$list = implode( ',', $set );
	$lump = $at-&gt;lookup( $list );
	foreach( $lump as $user ):
		$user = array(
			'handle' =&gt; $user-&gt;screen_name
		);
		$db-&gt;insert( $user, 'users' );
	endforeach;
endforeach;</string>
